.banner image{width: 100%; height: 300rpx;}
.pink-list-wrapper{padding: 0 20rpx;}
.pink-list-wrapper .item{margin-top: 10rpx; background-color: #fff;}
.pink-list-wrapper .item navigator{width: 100%; justify-content: space-between;}
.pink-list-wrapper .item image{width: 253rpx; height: 253rpx;}
.pink-list-wrapper .item .pro-info{width: 426rpx; padding-right: 30rpx; box-sizing: border-box; flex-direction: column;justify-content: space-around;}
.pink-list-wrapper .item .pro-info .title{display: -webkit-box; display:-moz-box; display:box;-webkit-box-orient: vertical; -moz-box-orient: vertical; -webkit-line-clamp: 2;-moz-line-clamp: 2;overflow: hidden;}
.pink-list-wrapper .item .pro-info .price-wrapper{font-size: 24rpx; color: #767676;}
.pink-list-wrapper .item .pro-info .price-wrapper .old-price{text-decoration: line-through; color: #aaa; margin-left: 10rpx;}
.pink-list-wrapper .item .pro-info .count-wrapper{position: relative; width: 100%;height:55rpx; background-color: #e9e9e9; font-size: 24rpx; justify-content: space-between; border-radius: 0 15px 15px 0; align-self: center;}
.pink-list-wrapper .item .pro-info .count-wrapper .text-bar{width: 145rpx; height: 100%;background-color: #ff3d3d; color: #fff; border-radius: 0 15px 15px 0; text-align: center; line-height: 55rpx;font-size:26rpx;}
.pink-list-wrapper .item .pro-info .count-wrapper .price{flex: 1; padding-right: 15rpx; text-align: right; line-height: 55rpx;}
.pink-list-wrapper .item .pro-info .count-wrapper .icon{width: 49rpx; height: 45rpx; background-image: url('data:image/png;base64,'); background-size: 100% 100%; position: absolute; left: 12rpx; top: -6rpx;}




